{
	"name": "etcopydata",
	"description": "SFDX Plugin to populate your scratch org and/or developer sandbox with data for multiple related sObjects.",
	"version": "0.4.3",
	"author": "Andres Perez @ElToroIT",
	"bugs": "https://github.com/eltoroit/ETCopyData/issues",
	"dependencies": {
		"@oclif/command": "1.5.6",
		"@oclif/config": "1.9.0",
		"@oclif/errors": "1.2.2",
		"@salesforce/command": "1.1.1",
		"bottleneck": "^2.17.1",
		"tslib": "1.9.3"
	},
	"devDependencies": {
		"@oclif/dev-cli": "1.19.5",
		"@oclif/plugin-help": "2.1.4",
		"@oclif/test": "1.2.2",
		"@salesforce/dev-config": "1.4.2",
		"@types/chai": "4.1.7",
		"@types/jsforce": "1.9.2",
		"@types/mocha": "5.2.5",
		"@types/node": "10.12.15",
		"chai": "4.2.0",
		"globby": "8.0.1",
		"mocha": "5.2.0",
		"nyc": "^13.3.0",
		"rimraf": "^2.6.3",
		"sinon": "7.2.2",
		"ts-node": "7.0.1",
		"typescript": "3.2.2"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/lib",
		"/messages",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/eltoroit/ETCopyData",
	"keywords": [
		"sfdx-plugin"
	],
	"license": "MIT",
	"oclif": {
		"commands": "./lib/commands",
		"topics": {
			"ETCopyData": {
				"description": "SFDX Plugin to populate your scratch org and/or developer sandbox with data for multiple related sObjects.y data from an org to a scratch org."
			}
		},
		"bin": "sfdx",
		"devPlugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "https://github.com/eltoroit/ETCopyData",
	"scripts": {
		"postpack": "rimraf oclif.manifest.json npm-shrinkwrap.json",
		"posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
		"prepack": "rimraf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
		"prepare": "rimraf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
		"test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md"
	}
}
